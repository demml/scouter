{
  "model": "gpt-4o-mini",
  "provider": "OpenAI",
  "version": "1.0.0",
  "parameters": ["text"],
  "response_type": "Text",
  "request": {
    "OpenAIRequest": {
      "messages": [
        {
          "OpenAIChatMessage": {
            "role": "User",
            "content": {
              "Text": {
                "text": "Analyze the sentiment of the following text and respond with only 'Positive', 'Negative', or 'Neutral':\n\n{text}"
              }
            }
          }
        }
      ],
      "system_instructions": [
        {
          "OpenAIChatMessage": {
            "role": "System",
            "content": {
              "Text": {
                "text": "You are a sentiment analysis expert. Respond only with 'Positive', 'Negative', or 'Neutral'."
              }
            }
          }
        }
      ],
      "model_settings": {
        "OpenAIChat": {
          "temperature": 0.1,
          "max_completion_tokens": 10
        }
      }
    }
  }
}
